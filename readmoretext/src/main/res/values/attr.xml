<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="ReadMoreTextView">
    <attr format="string" name="trimExpandedText" />
    <attr format="string" name="trimCollapsedText" />
    <attr format="integer" name="trimLength" />
    <attr format="boolean" name="showTrimExpandedText" />
    <attr format="color" name="colorClickableText" />
    <attr format="integer" name="trimLines" />
    <attr name="trimMode">
      <enum name="trimModeLine" value="0" />
      <enum name="trimModeLength" value="1" />
    </attr>
  </declare-styleable>

</resources>